<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="caffeine.console.command.question.username_question"
                 class="Caffeine\Console\Command\Question\UsernameQuestion"/>

        <service id="caffeine.console.command.question.timezone_question"
                 class="Caffeine\Console\Command\Question\TimezoneQuestion"/>

        <service id="caffeine.console.command.question.oauth_question"
                 class="Caffeine\Console\Command\Question\OauthQuestion"/>

        <service id="caffeine.storage.php_file_publisher"
                 class="Caffeine\Storage\PhpFilePublisher"/>

        <service id="caffeine.process.process_service" class="Caffeine\Process\ProcessService"/>
        <service id="caffeine.process.runtime.runtime_process" class="Caffeine\Process\Runtime\RuntimeProcess"/>

        <service id="caffeine.storage.configuration" class="Caffeine\Storage\Configuration"/>

        <service id="symfony.component.console.helper.question_helper"
                 class="Symfony\Component\Console\Helper\QuestionHelper"/>

        <service id="symfony.component.console.helper.helper_set"
                 class="Symfony\Component\Console\Helper\HelperSet"/>
    </services>
</container>
